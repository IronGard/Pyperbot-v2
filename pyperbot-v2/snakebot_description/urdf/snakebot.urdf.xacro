    </link>
    <joint name = "m1_prismatic_joint" type = "prismatic">
        <parent link = "base_link"/>
        <child link = "m1_bottom_bracket_one"/>
        <axis xyz = "1 0 0"/>
        <limit lower = "0" upper = "0.02" effort = "1" velocity = "0.01"/>
        <origin rpy = "0 0 0" xyz = "0 0 0"/>
    </joint>

