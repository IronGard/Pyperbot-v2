    <material name = "white">
        <color rgba = "1 1 1 1"/>
    </material>

    <!-- MODULE ONE-->
    <!-- First Joint -->
    <!-- Bottom Bracket-->
    <link name = "base_link">
        <visual>
            <origin rpy = "0 1.5708 0" xyz = "0 0 0"/>
            <geometry>
                <cylinder radius = "0.05" length = "0.02"/>
            </geometry>
            <material name = "white"/>
        </visual>
    </link>
    <link name = "m1_bottom_bracket_one">
        <visual>
